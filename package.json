{
  "name": "pp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest run --coverage",
    "test:unit-open": "vitest --ui",
    "test:integration": "cypress_mode=integration cypress run --browser chrome",
    "test:integration-open": "cypress_mode=integration cypress open"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vitest/ui": "^0.9.2",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "cypress": "^9.5.3",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.9.2",
    "vue-tsc": "^0.29.8"
  }
}
